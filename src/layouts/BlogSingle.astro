---
import Base from './Base.astro'
const props = Astro.props.content
// console.log(props)
const {title} = props
---

<Base {...props}>
  <article class="container--max-width container--base-padding blogPost">
    <h1 class="page__title">{title}</h1>
    <slot>Default Blog Content</slot>
  </article>
</Base>